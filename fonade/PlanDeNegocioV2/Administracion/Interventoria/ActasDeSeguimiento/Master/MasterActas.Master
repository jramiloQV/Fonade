                        <asp:CalendarExtender ID="CalendarFechaFinalVisita" runat="server"
                            Format="dd/MM/yyyy hh:mm tt"                            
                            TargetControlID="txtFechaFinalVisita" ClearTime="True" 
                            EnabledOnClient="True" TodaysDateFormat="" >
                        </asp:CalendarExtender>
                    </td>
                </tr>
                        <tr>
